// This code is created at IMT 7-7-2017 and this is a comment for a single line

/* Include stdio.h library 
To use printf function*/

#include<stdio.h> // 


/*

\n to start from new line
\t to add horizontal tab
\v to start after vertical tab

*/
main()

{
	printf("Hello, I am Ahmed\n");
	printf("Hello\nI am Ahmed\n");
	printf("Hello\t I am Ahmed\n");
	printf("Hello I am Ahmed \v");	
	
}
